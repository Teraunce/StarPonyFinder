{"name":"Joyous Victory","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<ol><li><p><span class=\"fontstyle2\">Joyous Victory: </span><span class=\"fontstyle3\">Every victory is cause for celebration and you take that to heart. Whenever you triumph over an encounter with a CR equal to your level or higher, you become inspired, gaining a +1 racial bonus to saving throws and skill checks for 1 minute. This racial trait replaces thick fur.</span></p></li></ol>","unidentified":""},"source":"","type":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"type":""},"area":{"value":0,"units":"","shape":"","effect":""},"range":{"value":null,"units":"","additional":"","per":""},"uses":{"value":0,"max":0,"per":null},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[],"requirements":"","recharge":{"value":null,"charged":false},"abilityMods":{"parts":[]}},"effects":[],"flags":{},"_stats":{"systemId":"sfrpg","systemVersion":"0.22.0","coreVersion":"10.291","createdTime":1672204776864,"modifiedTime":1673334646698,"lastModifiedBy":"7Idr7n4PmaIACOqY"},"_id":"7zMZ4gg6UZXmCL9R