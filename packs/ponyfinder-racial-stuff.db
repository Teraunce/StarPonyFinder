{"name":"Morning Prayer","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<ol><li><p><span class=\"fontstyle2\">Morning Prayer: </span><span class=\"fontstyle3\">Your people do not bow their heads to gods, but that does not mean they are without reverence. Your meager oblations to the forces of the universe keep you safe. Once per day, you may reroll a failed saving throw. This racial trait replaces thick fur.</span></p></li></ol>","unidentified":""},"source":"","type":"","activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"1 reroll","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[],"requirements":"","recharge":{"value":null,"charged":false},"abilityMods":{"parts":[]}},"effects":[],"flags":{},"_stats":{"systemId":"sfrpg","systemVersion":"0.22.0","coreVersion":"10.291","createdTime":1672204834982,"modifiedTime":1673334646699,"lastModifiedBy":"7Idr7n4PmaIACOqY"},"_id":"BYRUsBVQEmabu4L1","folder":