{"name":"Thick Tailed","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle3\">Your tail is unusually thick and heavy for a pony, granting a +2 trait bonus on Acrobatics<br>checks and making Acrobatics a class skill. This racial trait replaces low-light vision.</span></p>","unidentified":""},"source":"","type":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"type":""},"area":{"value":0,"units":"","shape":"","effect":""},"range":{"value":null,"units":"","additional":"","per":""},"uses":{"value":0,"max":0,"per":null},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[{"name":"Thick Tail","modifier":"2","type":"racial","effectType":"skill","valueAffected":"acr","enabled":true,"source":"","notes":"","modifierType":"constant","condition":"","subtab":"misc","max":2,"_id":"cd9804c3-e7c1-4283-90fd-5f853da3f8c0"}],"requirements":"","recharge":{"value":null,"charged":false},"abilityMods":{"parts":[]}},"effects":[],"flags":{},"_stats":{"systemId":"sfrpg","systemVersion":"0.22.0","coreVersion":"10.291","createdTime":1672256346752,"modifiedTime