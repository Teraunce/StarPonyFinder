{"name":"Cloven Watchfulness","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle2\">Cloven Watchfulness: </span><span class=\"fontstyle3\">Cloven gain a +2 racial bonus on Mysticism and Perception checks to find hidden objects (including traps and secret doors), determine whether food is spoiled, or identify a potion by taste.</span></p>","unidentified":""},"source":"","type":"","activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[{"name":"Watch Myst","modifier":"2","type":"racial","effectType":"skill","valueAffected":"mys","enabled":true,"source":"","notes":"Identify Potions by taste","modifierType":"constant","condition":"","subtab":"misc","max":2,"_id":"0d918344-b2e7-4fef-8da9-c366b0905105"},{"name":"Watch Perc","modifier":"2","type":"racial","effectType":"skill","valueAffected":"per","enabled":true,"source":"","notes