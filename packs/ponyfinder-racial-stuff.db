{"name":"Glowing Antennae","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle0\">Glowing: </span><span class=\"fontstyle2\">As an at-will supernatural ability, flutterponies can emit light from their antennae equivalent to a candle (5-foot radius, dim light). Igniting or dousing their light is a free action.</span></p>","unidentified":""},"source":"","type":"","activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"Until Ended","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"isActive":null,"ability":"","actionTarget":"","actionType":"util","attackBonus":0,"chatFlavor":"*lights up their antennae*","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":"","descriptor":"negate"},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[],"requirements":"","recharge":{"value":null,"charged":false},"abilityMods":{"parts":[]}},"effects":[],"flags":{},"_stats":{"systemId":"sfrpg","systemVersion":"0.22.0","coreVersion":"10.291","createdTime":1670946729295,"modifiedTime":1673334617983,"lastModifiedBy":"7Idr7n4PmaIACOqY"},"_id":"J1dmxtjVaxmudmfJ","folder":null,"sort":0,"ownership":{"default":0,"7Idr7n4PmaIACO