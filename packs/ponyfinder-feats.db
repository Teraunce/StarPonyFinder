{"name":"Painful Lesson","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle0\">Prerequisites: </span><span class=\"fontstyle2\">Con 13, zebra.<br></span><span class=\"fontstyle0\">Benefit: </span><span class=\"fontstyle2\">Any time you take an amount of damage equal<br>to three times your level in a single blow, you may make<br>a free skill check against the source<br>as appropriate, possibly remembering details you forgot<br>the last time you tried. This allows you to retry a check<br>to identify a spell or creature.</span> <br style=\" font-style: normal; font-variant: normal; font-weight: normal; letter-spacing: normal; line-height: normal; orphans: 2; text-align: -webkit-auto; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-size-adjust: auto; -webkit-text-stroke-width: 0px; \"></p>","unidentified":""},"source":"","type":"","activation":{"type":"none","cost":1,"condition":"take 3 times level damage in a single attack."},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":