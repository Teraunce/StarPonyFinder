{"name":"Spread Your Hooves","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle0\">Prerequisites: </span><span class=\"fontstyle2\">Ability to cast 4th level spells, antean, ponykind.<br></span><span class=\"fontstyle0\">Benefit: </span><span class=\"fontstyle2\">A number of times per day equal to 1 + your<br>Constitution modifier, you can enhance a spell you<br>are casting to make it widened, increasing the spell’s area by<br>50%. This has no effect on spells that aren't Burst, Emanation, or Spread-shaped.</span></p>","unidentified":""},"source":"","type":"","activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":1,"max":1,"per":"day"},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[],"requirements":"","recharge":{"value":null,"charged":false},"abilityMods":{"parts":[]}},"effects":[],"flags":{},"_stats":{"systemId":"sfrpg","systemVersion":"0.22.0","coreVersion":"10.291","createdTime":16718