{"name":"Fed by the Fires","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle0\">Prerequisites: </span><span class=\"fontstyle2\">Character level 9th, sun pony.<br></span><span class=\"fontstyle0\">Benefit: </span><span class=\"fontstyle2\">Whenever you take 5 or more points of lethal<br>fire damage, you accelerate (as the @UUID[Compendium.sfrpg.spells.PecEOjojT15BJKi9]{Haste} spell) for 1d4 rounds.</span> <br style=\" font-style: normal; font-variant: normal; font-weight: normal; letter-spacing: normal; line-height: normal; orphans: 2; text-align: -webkit-auto; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-size-adjust: auto; -webkit-text-stroke-width: 0px; \"></p>","unidentified":""},"source":"","type":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"type":""},"area":{"value":0,"units":"","shape":"","effect":""},"range":{"value":null,"units":"","additional":"","per":""},"uses":{"value":0,"max":0,"per":null},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[],"requirement