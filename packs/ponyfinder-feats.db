{"name":"Shared Breath","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle0\">Prerequisite: </span><span class=\"fontstyle2\">Sea horse.<br></span><span class=\"fontstyle0\">Benefit: </span><span class=\"fontstyle2\">With a simple kiss and a standard action,<br>you endow the target living creature with the ability to<br>breathe water for 1 hour per character level. You can use<br>this power a number of times equal to your Constitution<br>modifier + 1 per 5 levels you possess.</span> <br style=\" font-style: normal; font-variant: normal; font-weight: normal; letter-spacing: normal; line-height: normal; orphans: 2; text-align: -webkit-auto; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-size-adjust: auto; -webkit-text-stroke-width: 0px; \"></p>","unidentified":""},"source":"","type":"","activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"1 hour/level","units":""},"target":{"value":"kissed person","type":""},"area":{"value":null,"units":"personal","shape":"","effect":""},"range":{"value":null,"units":"touch","additional":"","per":""},"uses":{"value":1,"max":1,"per":"day"},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":true,"radiation":false,"scrying":false,"sense-depende