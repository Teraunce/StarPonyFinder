{"name":"Lost Among Peers (Combat)","type":"feat","img":"icons/svg/item-bag.svg","system":{"description":{"chat":"","gmnotes":"","short":"","value":"<p><span class=\"fontstyle0\">Prerequisite: </span><span class=\"fontstyle2\">Zebra.<br></span><span class=\"fontstyle0\">Benefit: </span><span class=\"fontstyle2\">You gain a +1 circumstance bonus to AC for<br>every other zebra within 10 feet of you. You do not gain<br>this bonus to AC against creatures that do not rely on sight.</span> <br style=\" font-style: normal; font-variant: normal; font-weight: normal; letter-spacing: normal; line-height: normal; orphans: 2; text-align: -webkit-auto; text-indent: 0px; text-transform: none; white-space: normal; widows: 2; word-spacing: 0px; -webkit-text-size-adjust: auto; -webkit-text-stroke-width: 0px; \"></p>","unidentified":""},"source":"","type":"","activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"isActive":null,"ability":null,"actionTarget":"","actionType":"","attackBonus":0,"chatFlavor":"","rollNotes":"","damageNotes":"","critical":{"parts":[],"effect":""},"damage":{"parts":[]},"formula":"","save":{"type":"","dc":null,"descriptor":""},"descriptors":{"acid":false,"air":false,"calling":false,"chaotic":false,"charm":false,"cold":false,"compulsion":false,"creation":false,"curse":false,"darkness":false,"death":false,"disease":false,"earth":false,"electricity":false,"emotion":false,"evil":false,"fear":false,"fire":false,"force":false,"good":false,"healing":false,"language-dependent":false,"lawful":false,"light":false,"mind-affecting":false,"pain":false,"poison":false,"polymorph":false,"radiation":false,"scrying":false,"sense-dependent":false,"shadow":false,"sonic":false,"summoning":false,"teleportation":false,"water":false},"modifiers":[{"name":"Camofl